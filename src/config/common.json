{
  "APP_NAME": "minterest-front-end",
  "DEVELOPMENT_KEYRING": true,
  "CUSTOM_TYPES": {
    "Address": "AccountId",
    "LookupSource": "AccountId",
    "CurrencyId": {
      "_enum": [
        "MINT",
        "DOT",
        "KSM",
        "BTC",
        "ETH",
        "MDOT",
        "MKSM",
        "MBTC",
        "METH"
      ]
    },
    "Operation": {
      "_enum": [
        "Deposit",
        "Redeem",
        "Borrow",
        "Repay"
      ]
    },
    "Pool": {
      "total_borrowed": "Balance",
      "borrow_index": "Rate",
      "total_insurance": "Balance"
    },
    "PoolUserData" : {
      "total_borrowed": "Balance",
      "interest_index": "Rate",
      "collateral": "bool"
    },
    "CurrencyIdOf": "CurrencyId",
    "Amount": "i128",
    "AmountOf": "Amount",
    "Rate": "FixedU128",
    "ControllerData": {
      "timestamp": "BlockNumber",
      "insurance_factor": "Rate",
      "max_borrow_rate":  "Rate",
      "collateral_factor": "Rate"
    },
    "PauseKeeper": {
      "deposit_paused": "bool",
      "redeem_paused": "bool",
      "borrow_paused": "bool",
      "repay_paused": "bool"
    },
    "MinterestModelData": {
      "kink": "Rate",
      "base_rate_per_block": "Rate",
      "multiplier_per_block": "Rate",
      "jump_multiplier_per_block": "Rate"
    },
    "CurrencyPair":  {
      "underlying_id": "CurrencyId",
      "wrapped_id": "CurrencyId"
    },
    "PoolState": {
      "exchange_rate": "Rate",
      "borrow_rate": "Rate",
      "supply_rate": "Rate"
    }
  },
  "RPC": {
    "controller": {
				"liquidityPoolState": {
					"description": "",
					"params": [
						{
							"name": "pool_id",
							"type": "CurrencyId"
						},
						{
							"name": "at",
							"type": "BlockHash",
							"isOptional": true
						}
					],
					"type": "PoolState"
      }
    }
  }
}
